	publisher = {{arXiv}},
	author = {Reichardt, Ben W. and Aasen, David and Chao, Rui and Chernoguzov, Alex and Dam, Wim van and Gaebler, John P. and Gresh, Dan and Lucchetti, Dominic and Mills, Michael and Moses, Steven A. and Neyenhuis, Brian and Paetznick, Adam and Paz, Andres and Siegfried, Peter E. and Silva, Marcus P. da and Svore, Krysta M. and Wang, Zhenghan and Zanner, Matt},
